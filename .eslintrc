{
  "extends": [
    "airbnb"
  ],
  "globals": {
    "afterAll",
    "beforeAll",
    "beforeEach",
    "describe",
    "expect",
    "fit",
    "it",
    "jasmine",
    "xit",
    "jest"
  },
  rules: {
    "import/named": 0,
    "no-underscore-dangle": 0,
    "import/no-dynamic-require": 0,
    "new-cap": 0,
    "consistent-return": 0,
    "import/no-mutable-exports": 0
  }
}